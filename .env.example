######################
# AUTHENTICATION
######################
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=your-email@example.com
N8N_BASIC_AUTH_PASSWORD=your-secure-password

######################
# DATABASE (AWS RDS)
######################
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=your-db-host.region.rds.amazonaws.com
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=your-db-username
DB_POSTGRESDB_PASSWORD=your-db-password
DB_POSTGRESDB_SSL_ENABLED=true
DB_POSTGRESDB_SSL_REJECT_UNAUTHORIZED=false
DB_POSTGRESDB_SSL_CA=/home/node/rds-combined-ca-bundle.pem

######################
# N8N INSTANCE CONFIG
######################
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_EDITOR_BASE_URL=http://your-server-ip:5678
WEBHOOK_URL=http://your-server-ip:5678

######################
# MISC (Recommended)
######################
N8N_ENCRYPTION_KEY=generate-a-random-64-character-encryption-key-here
N8N_COMMUNITY_PACKAGES_ALLOW_TOOL_USAGE=true